{
  "env": {
    "development": {
      "presets": [["es2015", { "modules": false }], "react", "stage-0"]
    },
    "test": {
      "presets": [
        "@babel/preset-env"
      ],
      "plugins": ["@babel/plugin-transform-runtime"]
    },
    "production": {
      "presets": [["es2015", { "modules": false }], "react", "stage-0"],
      "plugins": ["transform-react-remove-prop-types"]
    }
  }
}